#!/usr/bin/env bash
#Prints numbers from one to 20 with "badluck from china" 
#and "goodluck japan" inbetween
count=1
while [ $count -le 20 ]; do
    case $count in
        4)
            echo "4: bad luck from China"
            ;;
        9)
            echo "9: bad luck from Japan"
            ;;
        17)
            echo "17: bad luck from Italy"
            ;;
        *)
            echo "$count"
            ;;
    esac
    ((count++))
done
